{
  "manifest_version": 3,
  "name": "Focus Search",
  "version": "1.0.1",
  "description": "Focus on search input when loading a page",
  "permissions": [],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./index.js"
      ]
    }
  ]
}
